{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=(local);Initial Catalog=site_db;Trusted_Connection=True;Persist Security Info=True;TrustServerCertificate=true;"

  },
  "CookieAuth": {
    "CookieName": "CookieName",
    "ExpireTime": 60,
    "Secure": false,
    "HttpOnly": false
  },
  "Jwt": {
    "Key": "this is the custom Secret key for the ezeway application",
    "Issuer": "jwt_key",
    "audience": "public"
  },
  "Login_Temp": {
    "lt_enable": true,
    "lt_username": "username",
    "lt_pwd": "userpwd"
  },
  "Contact_Temp": {
    "ctactEnable": true,
    "ctactName": "contact name",
    "ctactEmail": "contact@contact.co.uk",
    "ctactPhone": "0123456789",
    "ctactSubject": "Test message",
    "ctactMessage": "This is a test message"
  },
  "Blog_Temp": {
    "blog_enable": true,
    "blog_title": "Test blog",
    "blog_content": "This is a test blog"
  },
  "FileInfo": {
    "FilePath": "site_data"
  },
  "AllowedHosts": "*"
}
